<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.gallery.GalleryFragment">

    <ImageView
        android:id="@+id/homepic"
        android:layout_width="394dp"
        android:layout_height="377dp"
        android:src="@drawable/homesmallers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.423" />

    <TextView
        android:id="@+id/homeModelText"
        android:layout_width="221dp"
        android:layout_height="64dp"
        android:layout_marginBottom="43dp"
        android:gravity="center"
        android:text="@string/homemodeltext"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/homepic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rightrooftext"
        android:layout_width="76dp"
        android:layout_height="39dp"
        android:gravity="center"
        android:rotation="25"
        android:text="@string/rightrooftext"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/doortext"
        app:layout_constraintEnd_toEndOf="@+id/homepic"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintStart_toStartOf="@+id/homepic"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.046" />

    <TextView
        android:id="@+id/livingroomtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/livingroomtext"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48" />

    <ImageView
        android:id="@+id/rightrooflamp"
        android:layout_width="55dp"
        android:layout_height="59dp"
        android:src="@drawable/baseline_lightbulb_24"
        app:layout_constraintBottom_toTopOf="@+id/doortext"
        app:layout_constraintEnd_toEndOf="@+id/homepic"
        app:layout_constraintHorizontal_bias="0.722"
        app:layout_constraintStart_toStartOf="@+id/homepic"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.137" />

    <ImageView
        android:id="@+id/livingroomlamp"
        android:layout_width="69dp"
        android:layout_height="68dp"
        android:src="@drawable/baseline_lightbulb_24"
        app:layout_constraintBottom_toTopOf="@+id/doortext"
        app:layout_constraintEnd_toEndOf="@+id/homepic"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="@+id/homepic"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.405" />

    <TextView
        android:id="@+id/lightText"
        android:layout_width="173dp"
        android:layout_height="31dp"
        android:gravity="center"
        android:text="TextView"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.872" />

    <SeekBar
        android:id="@+id/seekBarlamp"
        android:layout_width="191dp"
        android:layout_height="45dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />

    <ImageView
        android:id="@+id/leftrooflamp"
        android:layout_width="58dp"
        android:layout_height="59dp"
        android:src="@drawable/baseline_lightbulb_24"
        app:layout_constraintBottom_toTopOf="@+id/doortext"
        app:layout_constraintEnd_toEndOf="@+id/homepic"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="@+id/homepic"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.14" />

    <TextView
        android:id="@+id/leftrooftext"
        android:layout_width="85dp"
        android:layout_height="54dp"
        android:gravity="center"
        android:rotation="340"
        android:text="@string/leftrooftext"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/doortext"
        app:layout_constraintEnd_toEndOf="@+id/homepic"
        app:layout_constraintHorizontal_bias="0.223"
        app:layout_constraintStart_toStartOf="@+id/homepic"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.025" />

    <TextView
        android:id="@+id/doortext"
        android:layout_width="189dp"
        android:layout_height="48dp"
        android:layout_marginTop="364dp"
        android:gravity="center"
        android:text="@string/door"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/lightText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homepic"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>